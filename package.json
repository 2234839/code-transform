{
	"name": "code-transform",
	"displayName": "code-transform",
	"description": "code transform",
	"version": "0.0.3",
	"publisher": "llej",
	"repository": "git@github.com:2234839/code-transform.git",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished",
		"onCommand:extension.transform",
		"onCommand:extension.setFun"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.transform",
				"title": "transform"
			},
			{
				"command": "extension.setFun",
				"title": "setFun"
			}
		],
		"configuration": {
			"type": "object",
			"title": "code-transform",
			"properties": {
				"表征转换.启用": {
					"type": "boolean",
					"default": false,
					"description": "启用表征转换，这会消耗一些性能"
				},
				"表征转换.转换规则": {
					"type": "array",
					"items": {
						"type": "string"
					},
					"description": "您看到的代码会经过这些转换规则呈现出不一样的效果，但不会实际变动您的代码"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"ts-loader": "^6.2.1",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.10"
	}
}