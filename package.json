{
	"name": "code-transform",
	"displayName": "code-transform",
	"description": "code transform",
	"version": "1.1.3",
	"publisher": "llej",
	"repository": "2234839/code-transform",
	"engines": {
		"vscode": "^1.87.0"
	},
	"scripts": {
		"vscode:package": "vsce package --no-dependencies --baseContentUrl https://raw.githubusercontent.com/2234839/code-transform/master/",
		"vscode:prepublish": "pnpm build",
		"vscode:publish": "vsce publish",
		"watch": "tsup watch",
		"build": "tsup"
	},
	"categories": [
		"Other",
		"Programming Languages"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.setFun",
				"title": "setFun",
				"category": "code-transform"
			},
			{
				"command": "extension.transform",
				"title": "transform",
				"category": "code-transform"
			}
		],
		"configuration": {
			"type": "object",
			"title": "code-transform",
			"properties": {
				"表征转换.启用": {
					"type": "boolean",
					"default": false,
					"description": "启用表征转换，这会消耗一些性能"
				},
				"表征转换.转换规则": {
					"type": "array",
					"items": {
						"type": "string"
					},
					"description": "[未实现]您看到的代码会经过这些转换规则呈现出不一样的效果，但不会实际变动您的代码"
				}
			}
		}
	},
	"extensionKind": [
		"ui",
		"workspace"
	],
	"devDependencies": {
		"@types/vscode": "^1.98.0",
		"tsup": "^8.4.0",
		"typescript": "^5.8.2"
	},
	"dependencies": {
		"gogocode": "^1.0.55"
	},
	"packageManager": "pnpm@10.6.1+sha512.40ee09af407fa9fbb5fbfb8e1cb40fbb74c0af0c3e10e9224d7b53c7658528615b2c92450e74cfad91e3a2dcafe3ce4050d80bda71d757756d2ce2b66213e9a3"
}
